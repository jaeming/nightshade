<div>
  <p>I am a child component: {msg}</p>
  <p>a prop: {someProp}</p>
  <small>another prop: *{hello}*</small>
  <button click="{increment}">increment parent's count</button>
</div>

<script>
  export default class Child {
    msg = 'CHILD HERE'

    // props can declared this way too
    hello
    increment

    // or they can be set in the constructor
    constructor ({ someProp }) {
      Object.assign(this, { someProp })
    }
  }
</script>
